{
    // VSCode Env Settings
    "editor.fontFamily": "Menlo, Monaco, 'Courier New', monospace, Meslo LG M DZ for powerline",
    "window.zoomLevel": 0,
    "explorer.confirmDragAndDrop": false,
    "editor.formatOnSave": true,
    // go user setting with gvm
    "go.formatTool": "goimports",
    "go.lintTool": "golint",
    "go.testTimeout": "30s",
    "go.useLanguageServer": true,
    "go.autocompleteUnimportedPackages": true,
    "go.gotoSymbol.includeGoroot": true
}